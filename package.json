{
  "name": "innerhtml",
  "displayName": "innerHTML Syntax Highlighting",
  "description": "Syntax highlighting for HTML inside of JavaScript innerHTML template literals",
  "version": "0.2.0",
  "author": "Nicol√°s Parada <parada.nicolas@outlook.com>",
  "publisher": "nicolasparada",
  "galleryBanner": {
    "color": "#000",
    "theme": "dark"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "innerhtml",
    "javascript",
    "html",
    "template",
    "literal"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "source.js",
          "source.js.jsx",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "text.html.basic"
        ],
        "scopeName": "inline.innerhtml",
        "path": "./syntaxes/innerhtml.json",
        "embeddedLanguages": {
          "meta.embedded.block.html": "html",
          "meta.embedded.block.css": "css",
          "source.css": "css"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicolasparada/vscode-innerhtml.git"
  },
  "homepage": "https://github.com/nicolasparada/vscode-innerhtml#readme"
}
